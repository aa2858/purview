{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "member-access": false,
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "member-variable-declaration"
    ],
    "no-unbound-method": true
  },
  "linterOptions": {
    "exclude": ["**/*.json"]
  }
}
